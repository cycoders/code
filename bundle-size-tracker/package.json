{
  "name": "bundle-size-tracker",
  "version": "0.1.0",
  "description": "Tracks JavaScript bundle sizes across git commits with regression detection and trend visualization",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "bundle-size-tracker": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.1",
    "gzip-size": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "tsx": "^4.7.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.1"
  },
  "keywords": [
    "bundle",
    "size",
    "tracker",
    "frontend",
    "performance",
    "regression",
    "javascript",
    "typescript",
    "webpack",
    "vite"
  ],
  "author": "Arya Sianati",
  "license": "MIT"
}