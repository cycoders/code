{
  "format_version": "1.1",
  "terraform_version": "1.5.0",
  "resource_changes": [
    {
      "address": "example_aws_instance.web",
      "type": "aws_instance",
      "name": "web",
      "provider_name": "reg.provider.com/hashicorp/aws",
      "change": {
        "actions": ["create"],
        "after": {"ami": "ami-123", "instance_type": "t3.micro"}
      }
    },
    {
      "address": "aws_s3_bucket.my-prod-bucket",
      "type": "aws_s3_bucket",
      "name": "my-prod-bucket",
      "provider_name": "reg.provider.com/hashicorp/aws",
      "change": {
        "actions": ["delete"],
        "before": {"acl": "private", "public_ip": false}
      }
    }
  ]
}