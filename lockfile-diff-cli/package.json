{
  "name": "lockfile-diff-cli",
  "version": "0.1.0",
  "description": "Intelligently diffs package lockfiles (npm/yarn) to highlight dependency changes",
  "type": "module",
  "bin": {
    "lockdiff": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["lockfile", "npm", "yarn", "diff", "dependencies", "semver"],
  "author": "Arya Sianati",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.15",
    "commander": "^12.1.0",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.7.4",
    "eslint": "^9.9.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.7.0",
    "vitest": "^2.1.3"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ]
}